{
  "$schema": "node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "4.3.1",
    "generators": { 
      "v2.0": { 
        "generatorName": "typescript-axios",
        "output": "#{cwd}/packages/#{name}",
        "glob": "./doc/1.0.0/*.{json,yaml}",
        "skipValidateSpec": true,
        "additionalProperties": {
          "npmName": "geoserver-client-#{name}",
          "supportsES6": "true",
          "withInterfaces": true
        }
      }
    }
  }
}
